local Message ="A_1"
local A_1Text ="Notify Join And Leave!                                                                              [BY: Snox6785]"
local A_2 ="All"

while true do
	wait(1)
	local Event = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest Event:FireServer(A_1Text, A_2)
	wait(0.5)
	A_1Text:Destroy()
	wait(0)
	A_2:Destroy()
end

wait(1)
loadstring(game:HttpGet(('https://raw.githubusercontent.com/ImSnox/Chat/main/Left'),true))()
wait(.001)
loadstring(game:HttpGet(('https://raw.githubusercontent.com/ImSnox/Chat2/main/Joined'),true))()
wait(.001)

